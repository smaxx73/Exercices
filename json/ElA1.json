      "type": "reponse",
      "value": "ouvert"
    },
    {
      "type": "question",
      "value": "$B=\\{(x,y)\\in \\R^2 \\mid 0\\leq x\\leq y\\}$ ;"
    },
    {
      "type": "reponse",
      "value": "fermé"
    },
    {
      "type": "question",
      "value": "$C=\\{(x,y)\\in \\R^2 \\mid \\vert x\\vert <1,\\; \\vert y\\vert \\leq\n\t1 \\}$"
